<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_app_placeholder"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_app_placeholder"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="33">
        <activity
            android:name="com.afundacion.inazumawiki.detalleSt.DetalleStActivity"
            android:exported="false" />
        <activity
            android:name="com.afundacion.inazumawiki.detalleClub.DetalleClubActivity"
            android:exported="false" />
        <activity
            android:name="com.afundacion.inazumawiki.detalleJugador.DetalleJugadorActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.afundacion.inazumawiki.register.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.afundacion.inazumawiki.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.afundacion.inazumawiki.pantallaInitial.InitialActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.afundacion.inazumawiki.launcher.LauncherActivity"
            android:exported="true"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.afundacion.inazumawiki.main.MainActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
    </application>

</manifest>